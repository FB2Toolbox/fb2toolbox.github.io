      </checkedFiles>
      <focusedFile>
        <command name="Открыть в Cool Reader 3" fileName="C:\Program Files (x86)\Cool Reader 3\cr3.exe" arguments="&quot;{0}&quot;" waitAndReload="false"/>
        <command name="Редактировать FB2 в FBE" fileName="C:\Program Files (x86)\FictionBook Editor\FBE.exe" arguments="&quot;{0}&quot;" onlyWithExtension=".fb2"/>
      </focusedFile>
    </commands>
