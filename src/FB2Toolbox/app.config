      <!--
      Список элементов, которые можно использовать в профилях для переименования файлов:
      (Title)
      (Genre)
      (AuthorFirstName)
      (AuthorLastName)
      (AuthorMiddleName)
      (AuthorFirstNameI)
      (AuthorLastNameI)
      (AuthorMiddleNameI)
      (TranslatorFirstName)
      (TranslatorLastName)
      (TranslatorMiddleName)
      (TranslatorFirstNameI)
      (TranslatorLastNameI)
      (TranslatorMiddleNameI)
      (SequenceName)
      (SequenceNr)
      (Lang)
      -->
      
      <!-- Пользовательские профили (размещайте свои профили здесь) -->

      <!-- profile name="Пример"
        path="(AuthorLastName)|, (AuthorFirstName)|\(SequenceName)"
        fileName="(AuthorLastName)| (AuthorFirstName)| - (SequenceName)| (SequenceNr)| - (Title)"
      /-->

      <!-- Окончание пользовательских профилей -->


      <profile name="Автор/Серия/Автор - Серия Номер - Название"
        path="(AuthorLastName)|, (AuthorFirstName)|\(SequenceName)"
        fileName="(AuthorLastName)| (AuthorFirstName)| - (SequenceName)| (SequenceNr)| - (Title)"
      />
      <profile name="Автор/Серия/Автор - Серия Номер - Название (пер Переводчик)"
        path="(AuthorLastName)|, (AuthorFirstName)|\(SequenceName)"
        fileName="(AuthorLastName)| (AuthorFirstName)| - (SequenceName)| (SequenceNr)| - (Title)| (пер (TranslatorLastName))"
      />
      <profile name="Жанр/Автор/Серия/Автор - Серия Номер - Название"
        path="(Genre)|\(AuthorLastName)|, (AuthorFirstName)|\(SequenceName)"
